void ex1(){
        char car1=0x24;
        char car2=0x0b;
        int valeur1=6;
        int valeur2=16;
        int valeur3=5;
        printf("%d",valeur1%valeur2*valeur3);
}

void ex6(){
        int x;
        printf("Donner un entier :");
        scanf("%d",&x);
        if(x==0){
                printf("Vous avez saisi : nul\n");
                }
        if(x==1){
                printf("Vous avez saisi : un\n");
                }
        if(x==2){
                printf("Vous avez saisi : deux\n");
                }
        else{
                printf("Vous avez saisi un autre nombre que deux\n");
                }
        getch();
}


void main(){
        ex1();
        getch();
}
